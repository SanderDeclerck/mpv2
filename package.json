{
  "name": "offlineapp",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"npm run app:dev\" \"npm run serve\" \"npm run test:delayed\"",
    "app:dev": "cd ./app && npm run dev",
    "serve": "cd ./app && npm run serve",
    "test": "cd ./app && npm run test",
    "test:delayed": "cd ./app && sleep 3 && npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.0",
    "typescript": "^5.2.2"
  }
}
