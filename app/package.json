{
  "name": "mpv2",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "nodemon -w **/server/**/* -w **mock.ts -e ts -x \"ts-node --project tsconfig.server.json src/server/index.ts\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:contract": "vitest --config ./vitest.contract.config.ts",
    "test:contract:delayed": "sleep 2 && npm run vitest",
    "analyze": "vite-bundle-visualizer -t sunburst"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@tanstack/react-query": "^4.35.3",
    "@tsconfig/node12": "^12.1.0",
    "@tsconfig/node16": "^16.1.1",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.279.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.1",
    "vite-bundle-visualizer": "^0.10.0",
    "vitest": "^0.34.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@types/node": "^20.6.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "vite": "^4.4.5"
  }
}
